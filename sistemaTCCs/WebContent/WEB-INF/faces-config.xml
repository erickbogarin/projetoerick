<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" >

<managed-bean>
	<managed-bean-name>UsuarioLogin</managed-bean-name>
	<managed-bean-class>com.sistema.usuario.UsuarioLogin</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<navigation-rule>
	<from-view-id>/pages/public/login.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>primeiroAcesso</from-outcome>
		<to-view-id>/pages/protected/coordenador/primeiroAcesso.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>

</faces-config>